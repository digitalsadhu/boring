{
  "name": "boring",
  "version": "1.0.0",
  "description": "Takes care of the boring job of setting up finn node projects",
  "main": "index.js",
  "bin": "index.js",
  "dependencies": {
    "npm-install-package": "^2.1.0",
    "ora": "^1.3.0"
  },
  "devDependencies": {
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^4.8.0",
    "eslint-config-finn": "^3.0.0",
    "eslint-config-finn-prettier": "^3.0.1",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^4.2.3",
    "prettier": "^1.7.4"
  },
  "scripts": {
    "lint:format": "eslint --fix .",
    "lint": "eslint .",
    "test": "jest --coverage",
    "precommit": "lint-staged",
    "cm": "git-cz"
  },
  "author": "Richard Walker <digitalsadhu@gmail.com>",
  "license": "MIT",
  "jest": {
    "clearMocks": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/eslint --fix --config ./.eslintrc",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}